
datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}



model CoinAdded {
  dbId String @id @unique @default(uuid())
  coin_type Json
  price_feed_index Int
  decimals Int
}

model CoinRemoved {
  dbId String @id @unique @default(uuid())
  coin_type Json
}

model CollateralRedeemed {
  dbId String @id @unique @default(uuid())
  coin_type Json
  amount String
}

model DSCBurned {
  dbId String @id @unique @default(uuid())
  user String
  amount String
  position_id String
}

model LiquidationBonusUpdated {
  dbId String @id @unique @default(uuid())
  old_bonus String
  new_bonus String
}

model LiquidationThresholdUpdated {
  dbId String @id @unique @default(uuid())
  old_threshold String
  new_threshold String
}

model MinHealthFactorUpdated {
  dbId String @id @unique @default(uuid())
  old_min_health_factor String
  new_min_health_factor String
}

model NewDepositMade {
  dbId String @id @unique @default(uuid())
  coin_type Json
  amount String
}

model NewPositionCreated {
  dbId String @id @unique @default(uuid())
  new_position_id String
}

model Option {
  dbId String @id @unique @default(uuid())
  vec Json[]
}

model OracleHolderChanged {
  dbId String @id @unique @default(uuid())
  old_oracle_holder_id String?
  new_oracle_holder_id String
}

model PositionLiquidated {
  dbId String @id @unique @default(uuid())
  liquidated_user String
  liquidator String
  debt_covered String
  collateral_type Json
  collateral_amount String
}

model PrecisionUpdated {
  dbId String @id @unique @default(uuid())
  old_precision String
  new_precision String
}

model PriceFetched {
  dbId String @id @unique @default(uuid())
  coin_type Json
  price String
  decimal Int
  round String
}

model TypeName {
  dbId String @id @unique @default(uuid())
  name String
}


model cursor {
  id        String @id
  eventSeq  String
  txDigest  String
}